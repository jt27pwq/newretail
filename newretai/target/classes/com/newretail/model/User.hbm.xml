<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.newretail.model">
	<class name="User" table="User">
		<id name="keyword" column="User_Id">
			<generator class="native"/>
		</id>
		<property name="username" column="username" />
		<property name="nickname" column="nickname"/>
		<property name="password" column="password"/>
		<property name="type" column="type"/>
		<property name="gender" column="gender"/>
		<property name="address" column="adress"/>
		<property name="cardId" column="cardId"/>
		<property name="mphone" column="mphone"/>
		<property name="status" column="statu"/>
	</class>	
</hibernate-mapping>