<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.newretail.model">
	<class name="ShoppingCart" table="shopping_cart">
		<id name="id" column="keyword">
			<generator class="native"/>
		</id>
		<property name="unitCost" column="unit_cost"/>
		<property name="sCount" column="s_count"/>
		<property name="singleTotalPrice" column="single_totalprice"/>
		<property name="totalPrice" column="totalprice"/>
	</class>	
</hibernate-mapping>